generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model items {
  id                String          @id           @default(uuid())
  name              String          
  description       String
  UseOf             String
  url               String
  shopUrl           String?
  price             Int?                       
}